{
    "app":{
        "cfengine":"boxlang@be",
        "serverHomeDirectory":".engine/boxlang"
    },
    "name":"cborm-boxlang@1",
    "force":true,
    "openBrowser":false,
    "web":{
        "directoryBrowsing":true,
        "http":{
            "port":"60299"
        },
        "rewrites":{
            "enable":"true"
        },
        "webroot":"test-harness",
        "aliases":{
            "/moduleroot/cborm":"./"
        }
    },
    "JVM":{
        "heapSize":"1024",
        "javaVersion":"openjdk21_jre",
        "args":"-agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=8888 -Dboxlang.debugMode=true"
    },
    "cfconfig":{
        "file":".cfconfig.json"
    },
    "env":{
        "BOXLANG_DEBUG":true
    },
    "scripts":{
        "onServerInitialInstall":"install bx-mysql,bx-derby,bx-compat-cfml@be,bx-orm@be --noSave"
    },
    "-trace":true
}
